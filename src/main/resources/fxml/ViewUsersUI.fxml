<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.hms.user.controller.ViewUsersController"
            prefWidth="600.0" prefHeight="400.0" stylesheets="@/styles/app.css">
    <children>
        <TableView fx:id="usersTable" layoutX="10.0" layoutY="10.0" prefWidth="580.0" prefHeight="380.0">
            <columns>
                <TableColumn fx:id="idColumn" text="ID" prefWidth="70.0"/>
                <TableColumn fx:id="usernameColumn" text="Username" prefWidth="180.0"/>
                <TableColumn fx:id="roleColumn" text="Role" prefWidth="150.0"/>
                <TableColumn fx:id="updateColumn" text="Update" prefWidth="90.0"/>
                <TableColumn fx:id="deleteColumn" text="Delete" prefWidth="90.0"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>

